#:import BottomNav libs.components.bottom_nav.BottomNav
#: import FadeTransition kivy.uix.screenmanager.FadeTransition

<Conversation>:
    
    MDScreen:
        name: 'conversation'
        ScreenManager:
            id: screen_manager
            transition: FadeTransition()
        MDFloatLayout:
            md_bg_color: rgba(246,250,255,255)
            # height: 720
            MDIconButton:
                on_press: app.root.current = 'home'
                icon : "arrow-left"
                pos_hint: {"center_y": .95}
                user_font_size: "20sp"
                theme_text_color: "Custom"
                text_color: rgba(71, 92, 119, 255)

            MDIconButton:
                icon : "dots-vertical"
                pos_hint: {"center_x":.93,"center_y": .95}
                user_font_size: "20sp"
                theme_text_color: "Custom"
                text_color: rgba(71, 92, 119, 255)
        
            MDLabel:
                text: "ChatRoom"
                font_name:"instagram\\assets\\fonts\\BabyBonita.ttf"
                font_size: "26sp"
                pos_hint: {"center_x":.78,"center_y":.95}
                color: rgba(71,92,119,255)
            MDTextField:
                id: search_field
                hint_text: "Search friend"
                size_hint: .85, .89
                pos_hint: {"center_x": .5}
                # height: "48dp"
                # elevation: 10
                md_bg_color: 1, 1, 1, 1
                radius: [15]
                icon_left: "instagram/assets/images/seach.jfif"
                icon_left_color: 0, 0, 0, 1
                padding_x: "15dp"
                cursor_color: 0, 0, 0, 1
                cursor_width: "2sp"
                multiline: False
                background_color: 0, 1, 1, 0
                line_color_normal: 0, 0, 0, 1
                line_color_focus: 0, 0, 1,1
                # font_name: "instagram/assets/fonts/Quandary"
                font_size: "14sp"
                hint_text_color: (195/255, 208/255, 231/255, 1)
            # SearchBar:
            #     size_hint: .85,.89
            #     pos_hint: {"center_x" : .5, "center_y":.78}
            #     elevation:10
            #     md_bg_color: 1,1,1,1
            #     radius: [15]
            #     Image:
            #         source:"instagram/assets/images/seach.jfif"
            #         pos_hint:{"center_x":.13,"center_y":.48}
            #     TextInput:
