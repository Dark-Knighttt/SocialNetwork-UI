                do_scroll_y: True
                do_scroll_x: False
                size_hint_y: .72
                pos_hint: {"center_x":.5,"y":0}
                bar_width: 0
                GridLayout:
                    size_hint_y :None
                    row_default_height: 60
                    height: self.minimum_height
                    cols:1
                    spacing:15,10
                    padding:15,10
                    MDFloatLayout:
                        Circle:
                            radius: [50, ]
